{
  "directories": ["~/bin", "~/.jupyter", "~/.sshrc.d"],

  "link": {
    "zshrc":  "~/.zshrc",
    "zshenv": "~/.zshenv",
    "zitrc": "~/.zitrc",
    "antigenrc": "~/.antigenrc",
    "zimrc": "~/.zimrc",
    "shell/bashrc": "~/.bashrc",
    "shell/inputrc": "~/.inputrc",
    "shell/aliases.sh": "~/.aliases.sh",
    "sshrc": "~/.sshrc",
    "shell":  "~/.sshrc.d/shell",
    "dotvim/vimrc_minimal": "~/.sshrc.d/vimrc_minimal",
    "Gemfile": "~/Gemfile",
    "dotvim/gvimrc":  "~/.gvimrc",
    "dotvim/":    "~/.vim",
    "ackrc":  "~/.ackrc",
    "agignore": "~/.ignore",
    "ctags":  "~/.ctags",
    "git/gitconfig":  "~/.gitconfig",
    "git/gitattributes":  "~/.gitattributes",
    "git/gitignore":  "~/.gitignore",
    "jupyter/ipynb_drop_output.py": "~/bin/ipynb_drop_output.py",
